                %tr
                  %td{:style=>"text-align:right;"}
                    = number_with_delimiter(@vp['vno']['rua'])
                  %td{:style=>"text-align:right;"}
                    = "&mdash;".html_safe
                  %td Non-participating voters, registration update request accepted
                %tr
                  %td{:style=>"text-align:right;"}
                    = number_with_delimiter(@vp['vno']['rur'])
                  %td{:style=>"text-align:right;"}
                    = "&mdash;".html_safe
